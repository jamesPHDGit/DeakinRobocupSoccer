parametercommands = ["Say", "LearnFace"]

class ControlHandler():

    def handleRequest (self):
        while self.running:
            self.boxObject.log ("Handling connection")
            self.servSock.listen(1)
            self.conn, self.addr = self.servSock.accept()
            self.conn.send ("Ready. Send instruction > ")
            message = self.conn.recv (1024)
            self.conn.close ()
            self.boxObject.log ("Got message " + message)
            #self.server.boxObject.output_Say ("hello")
            #self.server.boxObject.raiseEvent ("output_Say", "got message")
            self.boxObject.log ("Action2 " + message)
            self.handleMessage (message)
        self.servSock.close ()
        self.boxObject.log ("Shutting down network")

    def __init__ (self, box):
        self.running = True
        self.boxObject = box
        self.boxObject.log ("Server starting")
        self.servSock = socket.socket (socket.AF_INET, socket.SOCK_STREAM)
        self.servSock.setsockopt (socket.SOL_SOCKET, socket.SO_REUSEADDR, 1)
        self.servSock.bind (('', 30006))
        t = threading.Thread (target=self.handleRequest)
        t.start()
        #self.boxObject.output_Say ("ready to run")

    def handleMessage (self, message):
        action = message.strip ().split (' ', 1)
        parameters = ""
        if len (action) > 1:
            parameters = action[1]

        self.boxObject.log ("Action " + action[0])

        if action[0] == "Stand":
            self.boxObject.log ("Action4 " + action[0])
            self.boxObject.output_Stand ()
        elif action[0] == "Sit":
            self.boxObject.output_Sit ()
        elif action[0] == "Forward":
            self.boxObject.output_Forward ()
        elif action[0] == "Backward":
            self.boxObject.output_Backward ()
        elif action[0] == "Left":
            self.boxObject.output_Left ()
        elif action[0] == "Right":
            self.boxObject.output_Right ()
        elif action[0] == "Stop":
            self.boxObject.output_Stop ()
        elif action[0] == "RecogniseFace":
            self.boxObject.output_RecogniseFace ()
        elif action[0] == "TaiChi":
            self.boxObject.output_TaiChi ()
        elif action[0] == "HelloWave":
            self.boxObject.output_HelloWave ()
        elif action[0] == "WipeForehead":
            self.boxObject.output_WipeForehead ()
        elif action[0] == "Record":
            self.boxObject.output_Record ()
        elif action[0] == "Playback":
            self.boxObject.output_Playback ()
        elif action[0] == "Speech":
            self.boxObject.output_Speech ()
        elif action[0] == "Say":
            #self.boxObject.log ("Action3 " + action[0])
            self.boxObject.output_Say (parameters)
        elif action[0] == "LearnFace":
            self.boxObject.output_LearnFace (parameters)
        elif action[0] == "FriendTracker":
            self.boxObject.output_FriendTrackerDemo ()
        elif action[0] == "NaoMark":
            self.boxObject.output_NaoMark ()
        elif action[0] == "Bumpers":
            self.boxObject.output_TestBumpers ()

        self.boxObject.output_RequestComplete ()

class MyClass(GeneratedClass):

        #self.onStopped() #activate the output of the box
        fl = os.popen ("/sbin/ifconfig | grep 'inet addr' | grep -v '127.0.0.1' | tr -s ' ' ' ' | cut -f2 -d: | cut -f1 -d' ' | tr '\n' ','")
